package com.Valera0823.Javabasico;
import java.util.Random;
import java.util.Scanner;
import java.math.*;
public class jv {

	static double GetRandom(double _a , double _b) {
		  return Math.floor(Math.random() * (_a - _b)) + _b;
		}
	public static void main(String[] args) {
		// TODO Auto-generated method stub
		
		String entradaTeclado = "";

        @SuppressWarnings("resource")
		Scanner entradaEscaner = new Scanner (System.in);
        entradaTeclado = entradaEscaner.nextLine (); 
        System.out.println("Dame el valor maximo :");
        double value = Double.parseDouble(entradaTeclado);
       
		 System.out.println(GetRandom(value,6));
		 
	}

}
